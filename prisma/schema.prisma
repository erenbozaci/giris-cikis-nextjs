// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
}

model Attendance {
  id    Int      @id @default(autoincrement())
  date  DateTime
  name  String
  giris String
  cikis String
  izin  Int      @default(0)
}
